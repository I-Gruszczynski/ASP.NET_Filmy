@model IEnumerable<ASP.NET_20_02_2023.Models.Kategoria>
@{
}
<header class="menu-bar text-center mt-3 mb-3">
    <nav class="menu">
        @foreach(var kategoria in Model)
        {
        <a href="@Url.Action("Lista", "Filmy", new {nazwaKategorii = kategoria.Nazwa.ToUpper()})" class="m-4 mt-5 mb-5">@kategoria.Nazwa</a>
        }
    </nav>
</header>